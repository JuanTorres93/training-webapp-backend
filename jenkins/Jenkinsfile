pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh '''
                    ./jenkins/build/build.sh
                '''
            }
        }
        stage('Test') {
            steps {
                echo 'Launching from git push..'
                echo 'Testing..'
            }
        }
        stage('Create Docker Image') {
            steps {
                echo 'Creating Docker image..'
            }
        }
        stage('Push Docker Image') {
            steps {
                echo 'Pushing Docker image..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
